<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module SYSTEM "http://google-web-toolkit.googlecode.com/svn/trunk/distro-source/core/src/gwt-module.dtd">

<module rename-to='rtpstat'>
	<inherits name='com.google.gwt.user.User' />
	<inherits name='com.google.gwt.resources.Resources' />
	<inherits name='com.google.gwt.user.theme.chrome.Chrome' />

	<entry-point class='org.ugr.rtpstat.client.Rtpstat' />

	<source path='client' />

	<replace-with class="org.ugr.rtpstat.client.uibinder.MainComposite">
		<when-type-is class="org.ugr.rtpstat.client.uibinder.MainCompositeBase" />
	</replace-with>
</module>
